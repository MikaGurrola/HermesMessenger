button
	display: inherit
	position: relative
	border-radius: 3px
	background-color: gray
	color: white
	margin: 0 auto
	border: none
	text-align: center
	text-decoration: none
	font-size: 16px
	padding: 5px 15px
	transition: all 0.5s cubic-bezier(.25,.8,.25,1)
	
